{
  "name": "Timed",
  "version": "1.0",
  "description": "Create nice timers, stopwatches and alarms!",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "32": "./src/img/test_32x32.png",
      "64": "./src/img/test_64x64.png"
    },
    "default_title": "Timed",
    "default_popup": "./src/popup.html"
  },
  "manifest_version": 2
}
